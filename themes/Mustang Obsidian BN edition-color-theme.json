{
	"tokenColors": [
		{
			"settings": {
				"background": "#202020",
				"foreground": "#c2c2c5",
				"selectionBorder": "#888888",
				"selectionCornerStyle": "square",
				"highlight": "#cccccc",
				"misspelling": "#FACB68"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#808080"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"foreground": "#BB9977"
			}
		},
		{
			"name": "Entity",
			"scope": "entity",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ccc"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"fontStyle": "",
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"fontStyle": "",
				"foreground": "#388B77"
			}
		},
		{
			"name": "Support",
			"scope": "support",
			"settings": {
				"fontStyle": "",
				"foreground": "#FAF4C6"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"foreground": "#bbb"
			}
		},
		{
			"name": "Invalid – Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#A2807E"
			}
		},
		{
			"name": "Invalid – Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#562D56",
				"foreground": "#F8F8F8"
			}
		},
		{
			"name": "-----------------------------------",
			"settings": {}
		},
		{
			"name": "♦ Embedded Source",
			"scope": "text source",
			"settings": {
				"background": "#252525"
			}
		},
		{
			"name": "♦ Embedded Source (Bright)",
			"scope": "text.html.ruby source",
			"settings": {
				"background": "#2A2A2A"
			}
		},
		{
			"name": "♦ Entity inherited-class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9B5C2E"
			}
		},
		{
			"name": "♦ String embedded-source",
			"scope": "string source",
			"settings": {
				"fontStyle": "",
				"foreground": "#DAEFA3"
			}
		},
		{
			"name": "♦ String constant",
			"scope": "string constant",
			"settings": {
				"foreground": "#44AA77"
			}
		},
		{
			"name": "♦ String.regexp",
			"scope": "string.regexp",
			"settings": {
				"fontStyle": "",
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "♦ String.regexp.«special»",
			"scope": [
				"string.regexp constant.character.escape",
				"string.regexp source.ruby.embedded",
				"string.regexp string.regexp.arbitrary-repitition"
			],
			"settings": {
				"foreground": "#44AA77"
			}
		},
		{
			"name": "♦ String variable",
			"scope": "string variable",
			"settings": {
				"foreground": "#bbb"
			}
		},
		{
			"name": "♦ Support.function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#ccc"
			}
		},
		{
			"name": "♦ Support.constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#ccc"
			}
		},
		{
			"name": "c C/C++ Preprocessor Line",
			"scope": "meta.preprocessor.c",
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "c C/C++ Preprocessor Directive",
			"scope": "meta.preprocessor.c keyword",
			"settings": {
				"foreground": "#44AA77"
			}
		},
		{
			"name": "✘ Doctype/XML Processing",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype entity",
				"meta.tag.sgml.doctype string",
				"meta.tag.preprocessor.xml",
				"meta.tag.preprocessor.xml entity",
				"meta.tag.preprocessor.xml string"
			],
			"settings": {
				"foreground": "#44AA77"
			}
		},
		{
			"name": "✘ Meta.tag.«all»",
			"scope": [
				"declaration.tag",
				"declaration.tag entity",
				"meta.tag",
				"meta.tag entity"
			],
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "✘ Meta.tag.inline",
			"scope": [
				"declaration.tag.inline",
				"declaration.tag.inline entity",
				"source entity.name.tag",
				"source entity.other.attribute-name",
				"meta.tag.inline",
				"meta.tag.inline entity"
			],
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "§ css tag-name",
			"scope": "meta.selector.css entity.name.tag",
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "§ css:pseudo-class",
			"scope": "meta.selector.css entity.other.attribute-name.tag.pseudo-class",
			"settings": {
				"foreground": "#bbb"
			}
		},
		{
			"name": "§ css#id",
			"scope": "meta.selector.css entity.other.attribute-name.id",
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "§ css.class",
			"scope": "meta.selector.css entity.other.attribute-name.class",
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "§ css property-name:",
			"scope": "support.type.property-name.css",
			"settings": {
				"foreground": "#bbb"
			}
		},
		{
			"name": "§ css property-value;",
			"scope": [
				"meta.property-group support.constant.property-value.css",
				"meta.property-value support.constant.property-value.css"
			],
			"settings": {
				"foreground": "#63BE78"
			}
		},
		{
			"name": "§ css @at-rule",
			"scope": "meta.preprocessor.at-rule keyword.control.at-rule",
			"settings": {
				"foreground": "#44AA77"
			}
		},
		{
			"name": "§ css additional-constants",
			"scope": [
				"meta.property-value support.constant.named-color.css",
				"meta.property-value constant"
			],
			"settings": {
				"foreground": "#90B080"
			}
		},
		{
			"name": "§ css constructor.argument",
			"scope": "meta.constructor.argument.css",
			"settings": {
				"foreground": "#8F9D6A"
			}
		},
		{
			"name": "⎇ diff.header",
			"scope": [
				"meta.diff",
				"meta.diff.header",
				"meta.separator"
			],
			"settings": {
				"background": "#0E2231",
				"fontStyle": "italic",
				"foreground": "#bbb"
			}
		},
		{
			"name": "⎇ diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"background": "#361f1f",
				"foreground": "#bbb"
			}
		},
		{
			"name": "⎇ diff.changed",
			"scope": "markup.changed",
			"settings": {
				"background": "#1d2638",
				"foreground": "#bbb"
			}
		},
		{
			"name": "⎇ diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"background": "#1d381e",
				"foreground": "#bbb"
			}
		},
		{
			"name": "Markup: List",
			"scope": "markup.list",
			"settings": {
				"foreground": "#FAF4C6"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#bbb"
			}
		}
	],
